{
  "name": "bimbus",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently --raw npm:dev:*",
    "dev:start": "ts-node-dev --respawn --transpile-only src",
    "start": "node build",
    "prebuild": "npm run lint && npm run clean",
    "build": "tsc",
    "test": "jest",
    "lint": "tslint src/**/*.ts",
    "clean": "rimraf build/"
  },
  "dependencies": {
    "@thream/socketio-jwt": "^2.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "js2xmlparser": "^4.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.3",
    "mongoose-paginate-v2": "^1.3.17",
    "slugify": "^1.5.0",
    "socket.io": "^4.0.1",
    "superstruct": "^0.15.1",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongoose-paginate-v2": "^1.3.9",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.1.3",
    "concurrently": "^6.0.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "ts-node-dev": "^1.1.6",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageReporters": [
      "text"
    ]
  }
}
