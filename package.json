{
  "name": "bimbus",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently --raw npm:dev:*",
    "dev:start": "ts-node-dev --respawn --transpile-only src",
    "start": "node build",
    "prebuild": "npm run lint && npm run clean",
    "build": "tsc",
    "test": "mocha --require ts-node/register --extension ts src/**/*.test.ts",
    "coverage": "nyc --all npm test",
    "lint": "tslint src/**/*.ts",
    "clean": "rimraf build/"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "js2xmlparser": "^4.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.2",
    "mongoose-paginate-v2": "^1.3.17",
    "slugify": "^1.5.0",
    "socket.io": "^4.0.0",
    "superstruct": "^0.15.1",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mocha": "^8.2.2",
    "@types/mongoose-paginate-v2": "^1.3.9",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.1.3",
    "chai": "^4.3.4",
    "concurrently": "^6.0.0",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.6",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  }
}
